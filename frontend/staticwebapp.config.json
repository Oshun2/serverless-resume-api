{
 "routes": [
   {
     "route": "/*",
     "allowedRoles": ["anonymous"]
   }
 ],
 "navigationFallback": {
   "rewrite": "/index.html"
 },
 "responseOverrides": {
   "404": {
     "rewrite": "/404.html"
   }
 },
 "globalHeaders": {
   "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload"
 }
}